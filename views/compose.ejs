<%- include("partials/header"); -%>

    <h1>Compose</h1>
    
    <form action="/compose" method="post">
        <div class="form-group">
          <label>Title</label>
          <input type="text" class="form-control" placeholder="Enter Title" name="postTitle">
        </div>
        <div class="form-group">
          <label>Description</label>
          <textarea class="form-control" rows="5" cols="30" placeholder="Enter Description" name="postBody"></textarea>
        </div>
        <button type="submit" class="btn btn-primary" name="button">Publish</button>
    </form>


<%- include("partials/footer"); -%>
